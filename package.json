{
  "name": "@timeax/cache-store",
  "private": false,
  "version": "0.0.2",
  "description": "A tiny cache store with TTL, pluggable drivers, and React hooks.",
  "license": "MIT",
  "author": "timeax",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timeax/cache-store.git"
  },
  "homepage": "https://github.com/timeax/cache-store#readme",
  "bugs": {
    "url": "https://github.com/timeax/cache-store/issues"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "node ./scripts/ensure-clean.cjs && npm run typecheck && npm run build",
    "postpublish": "node ./scripts/postpublish.cjs"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "@types/react": "^19.2.8",
    "react": "^19.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
